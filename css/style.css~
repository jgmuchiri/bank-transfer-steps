    background: none;
    background-color: #f0f0f1;
    -webkit-border-radius: 0;
    border-radius: 0;
    color: #000000;
    font-size: .9em !important;
    /* to override angular settings */
    height: 31px;
    /* makes selects the same size as inputs and small buttons */
    margin-bottom: .5em;
    min-height: 2em;
    position: relative;
    vertical-align: middle;
}

.ie9-select select {
